{
  "parser": "@typescript-eslint/parser",
  "extends": ["prettier"],
  "plugins": ["prettier"],
  "rules": {
    "no-console": "off",
    "unicorn/no-array-reduce": "off",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "lf",
        "singleQuote": true,
        "useTabs": false
      }
    ],
    "@typescript-eslint/no-var-requires": "off"
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
